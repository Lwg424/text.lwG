// 暴力列举法（3层循环）
//  时间复杂度 O(n^3) 

#include<stdio.h>
main()
{
	int a[7]={1,-5,8,3,-4,15,-8};
	int max = a[0];
	for(int i=0;i<7;i++)    // i表示每次sum从第a[i] 
	{
		for(int j=i+1;j<=7;j++)   // j表示连续相加j个数
		{
			int sum = 0;
			for(int k=i;k<j;k++)  // k用来表示数组的下标
			{
				sum += a[k];
			}
			if(sum>max)
			{
				max = sum;
			}
		}
	}
	printf("%d",max);
}
